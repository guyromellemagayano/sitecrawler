{
  "name": "frontend",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build-css": "cross-env NODE_ENV=production tailwindcss build css/app.css -o css/styles.css",
    "dev-css": "cross-env NODE_ENV=development tailwindcss build css/app.css -o css/styles.css",
    "next-dev": "NODE_OPTIONS='--inspect' next dev",
    "dev": "concurrently \"yarn run next-dev\" \"yarn run dev-css\"",
    "next-build": "next build --profile",
    "build": "concurrently \"yarn run next-build\" \"yarn run build-css\"",
    "start": "next start",
    "storybook": "start-storybook -p 6006 -c ./.storybook/ -s ./",
    "build-storybook": "build-storybook -c ./.storybook/ -s ./"
  },
  "browser": {
    "child_process": false
  },
