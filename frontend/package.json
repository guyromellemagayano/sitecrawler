{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build-css": "cross-env NODE_ENV=production tailwindcss build css/app.css -o css/styles.css",
    "dev-css": "cross-env NODE_ENV=development tailwindcss build css/app.css -o css/styles.css",
    "next-dev": "next dev",
    "dev": "concurrently \"npm run next-dev\" \"npm run dev-css\"",
    "next-build": "next build",
    "build": "concurrently \"npm run next-build\" \"npm run build-css\"",
    "start": "next start"
  },
  "browser": {
    "child_process": false
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.29",
    "@fortawesome/free-brands-svg-icons": "^5.13.1",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "@tailwindcss/ui": "^0.3.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-styled-components": "^1.10.7",
    "concurrently": "^5.2.0",
    "cross-env": "^7.0.2",
    "js-cookie": "^2.2.1",
    "moment": "^2.27.0",
    "next": "^9.4.4",
    "node-fetch": "^2.6.0",
    "postcss-cli": "^7.1.1",
    "postcss-import": "^12.0.1",
    "postcss-preset-env": "^6.7.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-copy-to-clipboard": "^5.0.2",
    "react-dom": "^16.13.1",
    "react-html-parser": "^2.0.2",
    "react-moment": "^0.9.7",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "styled-components": "^5.1.1",
    "styled-jsx-plugin-postcss": "^2.0.1",
    "swr": "^0.2.3",
    "window": "^4.2.7"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "babel-plugin-inline-react-svg": "^1.1.1",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-import-resolver-babel-module": "^5.1.2",
    "eslint-plugin-better-styled-components": "^1.1.2",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "tailwindcss": "^1.4.6"
  },
  "license": "MIT"
}
