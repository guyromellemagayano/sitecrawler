	"contributors": [
		"Domen Ipavec <domen.ipavec@gmail.com>"
	],
	"scripts": {
		"dev": "NODE_OPTIONS='--inspect' next dev",
		"build": "next build --profile --debug",
		"start": "next start",
		"lint": "next lint --dir ../frontend/{components,constants,helpers,hooks,pages,utils}",
		"format": "prettier --write .",
		"analyze": "cross-env ANALYZE=true next build",
		"analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
		"analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build"
	},
	"browser": {
		"child_process": false
	},
	"keywords": [
		"nextjs",
		"web",
		"crawl",
		"app",
		"web-crawl-app",
		"sitecrawler",
		"frontend"
