{
	"env": {
		"development": {
			"presets": [
				[
					"next/babel",
					{
						"preset-react": {
							"development": false
						}
					}
				]
			]
		}
	},
	"presets": [
		[
			"next/babel",
			{
				"preset-react": {
					"runtime": "automatic"
				}
			}
		]
	],
	"plugins": [
		"babel-plugin-macros",
		[
			"@babel/plugin-transform-react-jsx",
			{
				"runtime": "automatic"
			}
		],
		[
			"babel-plugin-styled-components",
			{
				"ssr": true
			}
		]
	]
}
