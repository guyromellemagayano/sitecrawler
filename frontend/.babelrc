{
    "presets": [
        [
            "next/babel",
            {
                "preset-react": {
                    "runtime": "automatic"
                }
            }
        ]
    ],
    "plugins": [
        "babel-plugin-macros",
        [
            "babel-plugin-styled-components",
            {
                "ssr": true,
                "displayName": false,
                "filename": false,
                "minify": true,
                "pure": true
            }
        ]
    ]
}
